//1st question
class Solution{

    sumElement(arr,n){
        var sum=0;
        for(var a = 0;a<n;a++){
              sum+=arr[a];
        }
        return sum;
    }

}


//2nd question
class Solution{

 isPalindrome(s) {
    
    const leng = s.length;

    
    for (let i = 0; i < leng / 2; i++) {

        if (s[i] !== s[leng - 1 - i]) {
            return '0';
        }
    }
    return '1';
}
}

